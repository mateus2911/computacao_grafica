<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
    <FileVersion major="1" minor="6" />
    <Project>
        <Option title="OpenGL_C" />
        <Option makefile_is_custom="1" />
        <Build>
            <Target title="Debug">
                <Option output="bin/Debug/OpenGL_C.exe" prefix_auto="1" extension_auto="1" />
                <Option working_dir="bin/Debug" />
                <Option object_output="obj/Debug/" />
                <Option type="1" />
                <Option compiler="gcc" />
                <Compiler>
                    <Add option="-g" />
                </Compiler>
                <Linker>
                    <Add library="freeglut" />
                    <Add library="opengl32" />
                    <Add library="glu32" />
                    <Add library="gdi32" />
                    <Add library="winmm" />
                    <Add directory="freeglut/lib/x64" />
                </Linker>
                <PostBuild>
                    <Command>copy "freeglut\bin\x64\freeglut.dll" "bin\Debug\freeglut.dll"</Command>
                </PostBuild>
            </Target>
        </Build>
        <Compiler>
            <Add directory="freeglut/include" />
        </Compiler>
        <Unit filename="main.c" />
        <Extensions>
            <code_completion />
            <envvars />
            <debugger />
        </Extensions>
    </Project>
</CodeBlocks_project_file>